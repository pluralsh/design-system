{
  "name": "pluralsh-design-system",
  "version": "1.84.0",
  "description": "Pluralsh Design System",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "start": "npm run storybook",
    "start:watch": "npm run storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src --ext ts,tsx,js,jsx",
    "clean": "rimraf storybook-static dist",
    "build": "npx tsc --declaration",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/react-transition-group": "^4.4.4",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.18.0",
    "eslint-config-pluralsh": "^3.1.0",
    "eslint-plugin-storybook": "^0.5.12",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "honorable": ">=0.116.0",
    "honorable-theme-default": ">=0.57.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "react-transition-group": ">=4.4.2"
  }
}
